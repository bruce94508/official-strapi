{
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "test": "jest --forceExit --detectOpenHandles",
    "strapi": "strapi",
    "prepare": "is-ci || yarn prepare:husky",
    "prepare:husky": "[ ! -d .git ] || { rm -rf .husky; husky install; husky add .husky/commit-msg 'yarn commitlint'; }",
    "commitlint": "commitlint --edit $1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "semantic-release": "^19.0.2",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".tmp",
      ".cache"
    ],
    "testEnvironment": "node"
  },
  "release": {
    "plugins": [
      "@semantic-release/changelog",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} [ci skip]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "tagFormat": "${version}"
  },
  "name": "official-strapi",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "dependencies": {
    "@strapi/strapi": "4.19.0",
    "@strapi/plugin-users-permissions": "4.19.0",
    "@strapi/plugin-i18n": "4.19.0",
    "@strapi/plugin-cloud": "4.19.0",
    "better-sqlite3": "8.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "5.3.4",
    "styled-components": "5.3.3"
  },
  "author": {
    "name": "A Strapi developer"
  },
  "strapi": {
    "uuid": "07b3d42b-adee-4ded-b211-9210de76044e",
    "template": "./maxwin-strapi-template"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
